#ifndef _VERSION_H_
#define _VERSION_H_

// version.h is automatically generated from version.in by a custom
// pre-build step that uses git's 'describe' function and
// replaces "GITREV" with current git commit info.

// Format: Major, Minor, Revision
//   Revision  = 0 for all Formally Released versions
//   Revision != 0 for all Intermediate versions

#define PRODUCTVER     @pwsafe_at_VERSION_MAJOR@, @pwsafe_at_VERSION_MINOR@, @pwsafe_at_REVISION@
#define STRATPRODUCTVER  "@pwsafe_at_VERSION_MAJOR@, @pwsafe_at_VERSION_MINOR@, @pwsafe_at_REVISION@, @pwsafe_at_VERSTRING@\0"

#define FILEVER        PRODUCTVER
#define STRATFILEVER   STRATPRODUCTVER

#endif // _VERSION_H_
